{
  "name": "student-agent",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:web": "expo start --web",
    "start:ios": "expo start --ios",
    "start:android": "expo start --android",
    "dev": "expo start --web",
    "dev:server": "tsx server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc && expo export --platform web && tsc -p tsconfig.server.json",
    "build:web": "expo export --platform web",
    "build:server": "tsc -p tsconfig.server.json",
    "build:all": "tsc && expo export --platform web && tsc -p tsconfig.server.json",
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/metro-config": "~0.18.11",
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@supabase/supabase-js": "^2.89.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "~51.0.0",
    "expo-constants": "~16.0.0",
    "expo-dev-client": "~4.0.0",
    "expo-status-bar": "~1.12.0",
    "express": "^5.2.1",
    "google-auth-library": "^10.5.0",
    "multer": "^2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "stripe": "^14.21.0",
    "supabase": "^2.71.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@expo/config": "~9.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/react": "~18.2.79",
    "@types/react-dom": "~18.2.25",
    "@types/react-native": "^0.72.8",
    "concurrently": "^9.2.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "tsx": "^4.7.0",
    "typescript": "~5.3.3"
  }
}
